{
    "manifest_version": 3,
    "name": "dp_downloader",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "favicon/favicon-16x16.png",
        "32": "favicon/favicon-32x32.png"
      }
    }
  }
  